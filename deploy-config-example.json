{
  "production": {
    "serverName": "serverName",
    "autoUpload": "Always",
    "autoUploadExternalChanges": true,
    "mappings": [
      {
        "deploy": "/",
        "local": "$PROJECT_DIR$",
        "web": "/"
      }
    ],
    "excludedLocal": [
      "$PROJECT_DIR$/.idea",
      "$PROJECT_DIR$/node_modules",
      "$PROJECT_DIR$/deploy-config.json"
    ],
    "excludedServer": []
  }
}


